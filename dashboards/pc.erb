<% content_for :title do %>Operations<% end %>

<div class="gridster">
<ul>
	
    <li data-row="1" data-col="1" data-sizex="3" data-sizey="2">
      <div data-id="dataFeedInventory" data-view="Highbar" data-title="Feed Inventory Trend" style="background-color:#f48024;"></div>
    </li>

	<li data-row="1" data-col="4" data-sizex="1" data-sizey="2">
	  <div data-id="casesDataOpsOpenToday" data-view="Hotness" data-title="Cases Open: DataOps" data-cool="5" data-warm="6"></div>
	</li>

    <li data-row="1" data-col="5" data-sizex="1" data-sizey="2">
      <div data-id="casesDataOpsClosedToday" data-view="Number" data-title="Cases Closed Today: DataOps" style="background-color:#96bf48;"></div>
      <!-- <i class="icon-heart icon-background"></i> -->
    </li>

    <li data-row="1" data-col="6" data-sizex="1" data-sizey="2">
      <div data-id="rotationDO" data-view="Text" style="background-color:#3183a6" data-title="" data-text="" data-moreinfo=""></div>
    </li>

	<li data-row="2" data-col="1" data-sizex="1" data-sizey="2">
	  <div data-id="monitorInStageWaiting" data-view="Hotness" data-title="InStage: Waiting" data-cool="250000" data-warm="400000"></div>
	</li>

	<li data-row="2" data-col="2" data-sizex="1" data-sizey="2">
	  <div data-id="monitorPropQTotalWaiting" data-view="Hotness" data-title="PropQueue: Total Waiting" data-cool="500000" data-warm="1000000"></div>
	</li>
		
	<li data-row="2" data-col="3" data-sizex="1" data-sizey="2">
	  <div data-id="monitorImagesTotalWaiting" data-view="Hotness" data-title="Images: Total Waiting" data-cool="300000" data-warm="500000"></div>
	</li>

	<li data-row="2" data-col="3" data-sizex="1" data-sizey="2">
	  <div data-id="monitorGQSJobsWaiting" data-view="Hotness" data-title="GQS Jobs: Total Waiting" data-cool="50" data-warm="100"></div>
	</li>

    <li data-row="2" data-col="4" data-sizex="2" data-sizey="2">
      <div data-id="casesDataOpsClosedTrend" data-view="Highbar" data-title="Cases Closed: Past 5 Days" style="background-color:#f48024;"></div>
    </li>

    <li data-row="3" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="issuesResolved2" data-view="Highbar" data-title="Data Issues" style="background-color:#f48024;"></div>
    </li>

    <li data-row="3" data-col="3" data-sizex="1" data-sizey="2">
      <div data-id="issuesResolved" data-view="Pie" data-title="Data Issues Resolved: YTD" style="background-color:#f39c12;"></div>
    </li>

    <li data-row="3" data-col="4" data-sizex="3" data-sizey="2">
      <div data-id="pivotaltrackerDOlist" data-title="Pivotal Tracker" data-view="Text" data-moreinfo="" style="background-color:#3498db;"></div>
    </li>

  </ul>
